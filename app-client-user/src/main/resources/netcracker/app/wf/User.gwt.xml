<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="user">
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.core.Core"/>
    <inherits name="com.google.gwt.uibinder.UiBinder"/>
    <inherits name="org.fusesource.restygwt.RestyGWT"/>
    <inherits name="com.google.gwt.resources.Resources"/>

    <inherits name="netcracker.app.wf.Back"/>

    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.activity.Activity"/>

    <entry-point class='netcracker.app.wf.client.user.UserEntryPoint'/>

    <replace-with class="netcracker.app.wf.client.user.ClientFactoryImpl">
        <when-type-is class="netcracker.app.wf.client.user.ClientFactory"/>
    </replace-with>
    <source path="client"/>
</module>