<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <div>
            <div class="tasksBox">
                <g:VerticalPanel ui:field="accordion"/>
            </div>
            <div class="taskScroll">
                <g:FlexTable ui:field="flexTable"/>
            </div>
            <div class="statusBox">
                <g:Label ui:field="statusField"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>