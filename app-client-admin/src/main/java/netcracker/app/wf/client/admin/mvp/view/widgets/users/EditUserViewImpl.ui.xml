<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <div class="addContainer">
            <table class="addTable">
                <tr>
                    <td><p>Login:</p></td>
                    <td><g:TextBox ui:field="loginField"/></td>
                    <td><g:Label ui:field="checkLoginLabel">            </g:Label> </td>
                </tr>
                <tr>
                    <td><p>Password:</p></td>
                    <td><g:PasswordTextBox ui:field="passwordField"/></td>
                    <td><g:Label ui:field="checkPasswordLabel">            </g:Label> </td>
                </tr>
                <tr>
                    <td><p>Re-enter password:</p></td>
                    <td><g:PasswordTextBox ui:field="passwordField2"/></td>
                    <td><g:Label ui:field="checkPasswordLabel2">            </g:Label> </td>
                </tr>
                <tr>
                    <td><p>Name:</p></td>
                    <td><g:TextBox ui:field="nameField"/></td>
                    <td><g:Label ui:field="checkNameLabel">            </g:Label> </td>
                </tr>
                <tr>
                    <td><p>Email:</p></td>
                    <td><g:TextBox ui:field="emailField"/></td>
                    <td><g:Label ui:field="checkEmailLabel">            </g:Label> </td>
                </tr>
                <tr>
                    <td><p>Address:</p></td>
                    <td><g:TextBox ui:field="addressField"/></td>
                    <td><g:Label ui:field="checkAddressLabel">            </g:Label> </td>
                </tr>
                <tr>
                    <td><p>Country:</p></td>
                    <td><g:TextBox ui:field="countryField"/></td>
                    <td><g:Label ui:field="checkCountryLabel">            </g:Label> </td>
                </tr>
                <tr>
                    <td><p>Java Skills:</p></td>
                    <td><g:TextBox ui:field="skillsField"/></td>
                    <td><g:Label ui:field="checkSkillsLabel">            </g:Label> </td>
                </tr>
                <tr>
                    <td><p>Role:</p></td>
                    <td><g:ListBox ui:field="roleField"/></td>
                    <td><g:Label ui:field="checkRoleLabel">            </g:Label> </td>
                </tr>
                <tr>
                    <td><g:Button ui:field="submitButton">Submit</g:Button> </td>
                    <td><g:Label ui:field="statusLabel"/> </td>
                </tr>
            </table>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>