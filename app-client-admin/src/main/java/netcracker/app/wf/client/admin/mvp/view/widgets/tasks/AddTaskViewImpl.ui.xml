<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <div class="addContainer">
            <table class="addTable">
                <tr>
                    <td><p>Title:</p></td>
                    <td><g:TextBox maxLength="50" ui:field="titleField"/></td>
                    <td><g:Label/></td>
                </tr>
                <tr>
                    <td><p>Description:</p></td>
                    <td><g:TextArea ui:field="descField"/></td>
                </tr>
                <tr>
                    <td><p>Target:</p></td>
                    <td><g:ListBox ui:field="targetField"/></td>
                </tr>
                <tr>
                    <td><g:Button ui:field="submitButton">Submit</g:Button> </td>
                    <td><g:Label ui:field="statusField"/> </td>
                </tr>
            </table>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>